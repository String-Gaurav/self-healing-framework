{
  "name": "self-healing-test-framework",
  "version": "1.0.0",
  "description": "AI-powered self-healing test automation framework for UI and API testing",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:ui": "npx playwright test --grep @ui",
    "test:api": "npx playwright test --grep @api",
    "test:healing": "npx playwright test --grep @healing",
    "install-browsers": "npx playwright install",
    "report": "npx playwright show-report",
    "debug": "npx playwright test --debug",
    "healing-demo": "node examples/healing-demo.js"
  },
  "keywords": [
    "test-automation",
    "self-healing",
    "playwright",
    "ai",
    "machine-learning",
    "api-testing",
    "ui-testing"
  ],
  "author": "Self-Healing Framework",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.40.0",
    "openai": "^4.20.0",
    "@anthropic-ai/sdk": "^0.24.3",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "ml-matrix": "^6.10.4",
    "ml-kmeans": "^6.0.0",
    "natural": "^6.7.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "ajv": "^8.12.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/lodash": "^4.14.200",
    "@types/uuid": "^9.0.6",
    "typescript": "^5.2.2",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
